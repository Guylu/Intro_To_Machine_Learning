\[
P^{2}=\sumjk(v_{j}v_{j}^{\T}\cdot\sumik v_{i}v_{i}^{\T})=\sumik v_{i}v_{i}^{\T}=P
\]

\end_inset


\end_layout

\begin_layout Quotation
(e) 
\begin_inset Formula 
\[
(I-P)P=(I-P)\sumik v_{i}v_{i}^{\T}=\sumik(I-P)v_{i}\cdot v_{i}^{\T}\overset{v_{i}\ eigenvector\ of\ value\ 1}{=}\sumik(v_{i}-v_{i})v_{i}^{\T}=0
\]

\end_inset



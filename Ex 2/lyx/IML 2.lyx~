\begin_layout Quotation
Let 
\begin_inset Formula $X\in\RR^{pXn}$
\end_inset

 be a matrix we will prove 
\begin_inset Formula $Ker(X^{T})\subset Ker(XX^{T})\wedge Ker(XX^{T})\subset Ker(X^{T})$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Ker(X^{T})\subset Ker(XX^{T}):
\]

\end_inset


\end_layout

\begin_layout Quotation
Let 
\begin_inset Formula $v\in Ker(X^{T})$
\end_inset

 then 
\begin_inset Formula $XX^{T}v=X\cdot0=0\ra v\in Ker(XX^{T})$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Ker(XX^{T})\subset Ker(X^{T}):
\]

\end_inset


\end_layout

\begin_layout Quotation
let 
\begin_inset Formula $v\in Ker(XX^{\T})\ra XX^{\T}v=0$
\end_inset

 lets look at 
\begin_inset Formula $0=v^{\T}XX^{\T}v=$
\end_inset


\end_layout

\begin_layout Quotation
\begin_inset Formula $(X^{\T}v)^{\T}(X^{\T}v)\ra X^{\T}v\overset{vector\ that\ orthogonal\ to\ itself\ is\ the\ 0\ vector}{=}0\ra v\in Ker(X^{\T})$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathscr{\textrm{\ensuremath{\mathcal{Q}uestion} }}2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename pasted12.png

\end_inset


\end_layout

\begin_layout Quotation
lets prove the statement:
\end_layout

\begin_layout Quotation
\begin_inset Formula 
\[
Im(A^{T})\subset Ker(A)^{\perp}:
\]

\end_inset


\end_layout

\begin_layout Quotation
let 
\begin_inset Formula $v\in Im(A^{T})$
\end_inset

 and let 
\begin_inset Formula $w\in Ker(A)$
\end_inset

 we need to prove that 
\begin_inset Formula $\li v,w\ri=0$
\end_inset

.
\end_layout

\begin_layout Quotation
we know that 
\begin_inset Formula $Aw=0$
\end_inset

 and that 
\begin_inset Formula $\E u,s.t\ v=A^{T}u=v$
\end_inset


\end_layout

\begin_layout Quotation
\begin_inset Formula $\li v,w\ri=\li A^{T}u,w\ri\overset{parseval}{=}(A^{T}u)^{T}w=u^{T}Aw\overset{w\in Ker(A)}{=}u^{T}\cdot0=0$
\end_inset


\end_layout

\begin_layout Quotation
\begin_inset Formula 
\[
Ker()^{\perp}\subset Im(A^{T}):
\]

\end_inset


\end_layout

\begin_layout Quotation
let 
\begin_inset Formula $v\in Ker(A)^{\perp}$
\end_inset

, we need to prove that 
\begin_inset Formula $\E u,s.t\ v=A^{T}u$
\end_inset


\end_layout

\begin_layout Quotation
suppose there is no 
\begin_inset Formula $u$
\end_inset

 s.t 
\begin_inset Formula $\forall w\in Ker(A)$
\end_inset

 it holds that 
\begin_inset Formula $\li A^{T}u,w\ri\neq0$
\end_inset


\end_layout

\begin_layout Quotation
from parseval 
\begin_inset Formula $\li A^{T}u,w\ri=u^{T}Aw=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathscr{\textrm{\ensuremath{\mathcal{Q}uestion} }}3$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename pasted13.png

\end_inset


\end_layout

\begin_layout Quotation
we know that the system has 
\begin_inset Formula $\infty$
\end_inset

 solution meaning there exists at least one solution w.
\end_layout

\begin_layout Quotation
so we know that 
\begin_inset Formula $y\in Im(A^{T})\ra$
\end_inset

from Q2 we know that 
\begin_inset Formula $Im(A^{T})=Ker(A)^{\perp}\ra y\in Ker(A)^{\perp}$
\end_inset


\end_layout

\begin_layout Quotation
\begin_inset Formula $\ra y\perp Ker(A)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathscr{\textrm{\ensuremath{\mathcal{Q}uestion} }}4$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename pasted14.png

\end_inset


\end_layout

\begin_layout Quotation
we know that 
\begin_inset Formula $XX^{T}w=Xy$
\end_inset

, now if 
\begin_inset Formula $XX^{T}$
\end_inset

is invertible then we can multiply from the left
\end_layout

\begin_layout Quotation
both sides by 
\begin_inset Formula $(XX^{T})^{-1}$
\end_inset

 and get 
\begin_inset Formula $w=(XX^{T})^{-1}Xy$
\end_inset

 as a unique solution to the equation
\end_layout

\begin_layout Quotation
\begin_inset space \space{}
\end_inset


\end_layout

\begin_layout Quotation
if however 
\begin_inset Formula $XX^{T}$
\end_inset

is invertible, we know that it has 
\begin_inset Formula $\infty$
\end_inset

 solutions 
\begin_inset Formula $\iff$
\end_inset


\begin_inset Formula $X^{T}y\perp Ker(X^{T}X)\overset{Q1}{=}Ker(X)$
\end_inset


\end_layout

\begin_layout Quotation
lemma:
\begin_inset Formula $X^{T}y\perp Ker(X)$
\end_inset

.
 proof: let 
\begin_inset Formula $v\in Ker(X)$
\end_inset

 lets look at 
\begin_inset Formula $\li X^{T}y,v\ri=y^{T}Xv=0$
\end_inset


\end_layout

\begin_layout Quotation
so we proved that 
\begin_inset Formula $X^{T}y\perp Ker(X)$
\end_inset

 which means that the system has 
\begin_inset Formula $\infty$
\end_inset

 solutions, as required.
\end_layout

\begin_layout Quotation
\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathscr{\textrm{\ensuremath{\mathcal{Q}uestion} }}5$
\end_inset


